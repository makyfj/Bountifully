{
  "name": "bountifully",
  "version": "1.0.0",
  "description": "MERN shopping",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makyfj/Bountifully.git"
  },
  "keywords": [
    "NodeJS",
    "Express",
    "React",
    "Redux",
    "MongoDB"
  ],
  "author": "makyfj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makyfj/Bountifully/issues"
  },
  "homepage": "https://github.com/makyfj/Bountifully#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.12",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  }
}
